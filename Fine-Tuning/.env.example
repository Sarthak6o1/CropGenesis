##############################################
# Google Cloud / Vertex AI Configuration File
# Copy this file to `.env` and fill in the real values.
##############################################

# Path to your Google Cloud service account key file.
# IMPORTANT:
# You must download the service-account.json from:
# Google Cloud Console → IAM & Admin → Service Accounts → Keys → Create Key (JSON)
GOOGLE_APPLICATION_CREDENTIALS=service-account.json

# Your Google Cloud project ID
# Example: my-project-id
GOOGLE_CLOUD_PROJECT_ID=YOUR_PROJECT_ID

# The region where your Vertex AI resources are hosted
# Example: us-central1, europe-west4, asia-south1
VERTEX_AI_REGION=YOUR_VERTEX_REGION

# The Vertex AI Endpoint ID where your model is deployed
VERTEX_AI_ENDPOINT_ID=YOUR_ENDPOINT_ID
